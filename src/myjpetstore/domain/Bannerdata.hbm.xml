<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class name="myjpetstore.domain.Bannerdata" table="bannerdata" lazy="false">
                <id name="favcategory" type="text" column="userid">
                        <generator class="assigned"/>
                </id>
                <property name="bannername" type="text" column="bannername" not-null="true" />
        </class>
</hibernate-mapping>