<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class name="myjpetstore.domain.Category" table="category" lazy="false">
                <id name="categoryId" type="text" column="catid">
                        <generator class="assigned"/>
                </id>
                <property name="name" type="text" column="name" not-null="true" />
                <property name="description" type="text" column="descn" not-null="true" />
        </class>
</hibernate-mapping>